/ {

	aliases {
		usb0 = &usb_dwc3_0;
		usb1 = &usb_dwc3_1;
	};
};

&usb3_0 {
	compatible = "simple-bus";
};

&usb_dwc3_0 {
	compatible = "fsl, imx8mq-dwc3";
};

&usb3_1 {
	compatible = "simple-bus";
};

&usb_dwc3_1 {
	compatible = "fsl, imx8mq-dwc3";
};
